.bottomSide {
  position: fixed;
  top: calc(100% - 50px);
  left: 0;
  width: 100%;
  height: 50px;
  z-index: 99999;
  background-color: #fff;
  text-align: center;
  border-top: 1px solid #1296db;
  display: flex;
}

.Hide {
  display: none;
}
.welcome{
  width:100%;
  height:100vh;
  position: absolute;
  background-color:#ffffff;
  opacity: 0.9;
  top:0;
  left: 0;
  
}
.fontweight{
  font-weight: bolder;
}
.note{
  width:100%;
  height:100vh;
  top:0;
  left: 0;
  position: absolute;
}
.default{
  border-radius: 20px;
  background-color:#1296db;
  width: 80%;
  height:80vh;
  border: 1px solid #888;
  margin:40px auto;
  z-index:99999;
}
.app_title{
  width:100%;
