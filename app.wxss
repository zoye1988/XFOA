.container {
  background-color: #f5f4f4;
  margin: 0px auto;
  text-align: center;
}
.page-radius{
  border-radius:5px;
  background-color:#fcfcfc;
  border:1px solid #888;
  padding:2px;
  height:20px;
  line-height: 20px;
  width:100px;
  margin:2px auto;
}
.page-radius2{
  border-radius:5px;
  background-color:#1296db;
  border:1px solid #888;
  padding:2px;
  height:20px;
  line-height: 20px;
  width:100px;
  margin:0px auto;
}
.page-warning {
  height: 20px;
  font-size: 8pt;
  text-align: center;
  line-height: 20px;
  color: #888;
}
button{
  margin:10px;
}
.page-switch{
  height:20px;line-height:20px;width:40px;border:1px solid #cdcdcd;border-radius:5px;margin:0 auto;margin-top:4px;
}

.page-body {
