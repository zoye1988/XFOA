@import "../../utils/weui.wxss";
.page-body{
  background-color:#ffffff;
}
.page-border{
  border-left:1px solid #cdcdcd;
}
.page-body text{
  color:black;
  line-height: 40px;
}
.page-bottom{
  border-bottom:1px solid #cdcdcd;
}
.options{
  color:black;
  height:30px;
  margin:0 auto;
  width:90%;
  line-height: 30px;
  border-bottom:1px solid #f2f2f2;
  font-size: 9pt;
}
.page-text{
  height:20px;
  line-height: 20px;
  overflow: hidden;
  font-size: 9pt;
}
.page-text-comfirm{
  font-size: 8pt;
  color:#1296db;
  border:1px solid #1296db;
  width:70%;
  margin:0 auto;
}
.page-text-refuse{
  font-size: 8pt;
  color:red;
  border:1px solid red;
  width:70%;
  margin:0 auto;
}
.page-text-waiting{
  font-size: 8pt;
  color:green;
  border:1px solid green;
  width:70%;
  margin:0 auto;
}
.page-items{
  height:90px;
  background-color:#ffffff;
  margin:2px;
  margin-bottom:5px;
  border-bottom:1px solid #cdcdcd;
}
.page-comfirm{
  height:30px;
  width:80px;
  margin:10px auto;
  line-height:30px;
  border-radius: 10px;
  color:white;
}
.page-refuse{
  height:30px;
  width:80px;
  line-height:30px;
  margin:10px auto;
  border-radius: 10px;
  color:white;
}
.page-item text{
  color:#888;
}
.page-item{
  margin-top:5px;
}
.page-head{
  width:100%;
  height:30px;
  background-color:#ffffff;
}
.page-head text{
  line-height: 30px;
  font-size: 10pt;
  font-weight: bold;
}
.tip{
  height:50px;
  width:50px;
  margin-top:10px;
}
.tip2{
  height:20px;
  width:20px;
  float:right;
  margin-top:5px;
}
.tip3{
  height:25px;
  width:25px;
  margin-left:-25px;
}
.tip4{
  height:40px;
  width:40px;
}
.page-files{
  margin-left:10px;
  display: inline;
}
.fileimg{
  width:80px;
  height:80px;
}
.page-title{
  line-height: 20px;
  font-size: 10pt;
  font-weight: bold;
}
.page-brief{
  height:20px;
  line-height: 20px;
  font-size: 9pt;
  overflow: hidden;
  color:#8a8a8a;
}