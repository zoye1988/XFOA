.container{
  
}
.page-body{
  height:40px;
  background-color:#ffffff;
  margin:2px;
  padding:5px;
  border:1px solid #cfcfcf;
  border-radius: 10px;
}
.page-body text{
  color:red;
}
.page-items{
  margin:1px;
  text-align: center;
  border-bottom:1px solid #ececec;
}
.page-items2{
  text-align: center;
  background-color:#1296db;
  
}
.page-item{
  height:70px;
  border-right:1px solid #ececec;
  margin:1px;
  margin-top:0;
  padding-top:10px;
  background-color:#ffffff;
}
.page-item2{
  height:80px;
  border-radius: 10px;
  margin:2px;
  margin-top:0;
  padding:0px 5px;
  color:#ffffff;
}
.page-head{
  width:100%;
  height:30px;
  background-color:#ffffff;
  color:#1296db;
  margin-bottom:5px;
}
.page-head text{
  line-height: 30px;
  font-size: 10pt;
  padding-left: 10px;
  
}
.tip{
  height:30px;
  width:30px;
  margin-top:5px;
}
.tip2{
  height:20px;
  width:15px;
  margin-top:10px;
}
.tip3{
  height:40px;
  width:40px;
}
.tip4{
  height:30px;
  width:30px;
  margin-top:15px;
}
.page-title{
  line-height: 20px;
  font-size: 9pt;
}
.page-brief{
  height:20px;
  line-height: 20px;
  font-size: 9pt;
  overflow: hidden;
  color:#8a8a8a;
}