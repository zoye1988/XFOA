.page-body{
  height:30px;
  background-color:#ffffff;
  margin:2px;
  margin-bottom:5px;
  padding:5px 0;
}
.page-border{
  border-left:1px solid #cdcdcd;
}
.page-body text{
  line-height: 40px;
}
.page-text{
  height:20px;
  line-height: 20px;
  overflow: hidden;
  font-size: 9pt;
}
.page-text-comfirm{
  font-size: 8pt;
  color:#1296db;
  border:1px solid #1296db;
  width:70%;
  margin:0 auto;
}
.page-text-refuse{
  font-size: 8pt;
  color:red;
  border:1px solid red;
  width:70%;
  margin:0 auto;
}
.page-text-waiting{
  font-size: 8pt;
  color:green;
  border:1px solid green;
  width:70%;
  margin:0 auto;
}
.fontcolor{
  font-weight: bold;
  color:#1296db;
}
.nofontcolor{
  color:#888;
}
.page-items{
  height:90px;
  background-color:#ffffff;
  margin:2px;
  margin-bottom:5px;
  border-bottom:1px solid #cdcdcd;
}
.page-comfirm{
  height:25px;
  width:80px;
  margin:5px auto;
  line-height:25px;
  border-radius: 10px;
  color:white;

}
.page-refuse{
  height:25px;
  width:80px;
  line-height:25px;
  margin:5px auto;
  border-radius: 10px;
  color:white;
}
.page-item text{
  color:#888;
}
.page-item{
  margin-top:5px;
}
.page-head{
  width:100%;
  height:30px;
  background-color:#ffffff;
  color:#1296db;
  margin-bottom:5px;
  border-bottom:1px solid #cdcdcd;
}
.page-head text{
  line-height: 30px;
  font-size: 10pt;
}
.tip{
  height:50px;
  width:50px;
  margin-top:10px;
}
.tip2{
  height:25px;
  width:25px;
}
.tip3{
  height:40px;
  width:40px;
  margin-top:15px;
}
.page-title{
  line-height: 20px;
  font-size: 10pt;
  font-weight: bold;
}
.page-brief{
  height:20px;
  line-height: 20px;
  font-size: 9pt;
  overflow: hidden;
  color:#8a8a8a;
}